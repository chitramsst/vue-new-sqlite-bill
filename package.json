    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-store": "^9.0.0",
    "express": "^4.19.2",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "mysql2": "^3.9.8",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
