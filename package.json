{
  "name": "vue-bill",
  "version": "0.0.0",
  "private": true,
  "main": "electron/electron.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "electron": " electron .",
    "electron:pack": "electron-builder --dir",
